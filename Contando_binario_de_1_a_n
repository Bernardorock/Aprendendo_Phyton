numero = int(input('Insert of number \n'))
juncao =''


for i in range(1,numero+1):
    print(f'Numero {i} ')
    while i > 0:
        coletando = i%2
        i = i//2

        juncao = juncao+(str(coletando))
        #print(juncao)
    v = int(juncao)
    for k in range(len(juncao)):
        binario = v%10
        v = v//10
        print(binario)
    juncao = ''  
